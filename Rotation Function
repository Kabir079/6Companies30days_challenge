class Solution:
    def maxRotateFunction(self, nums: List[int]) -> int:
        Sum = sum(nums)
        n = len(nums)
        ans = val = sum(i*val for i, val in enumerate(nums))
        for i in range(n-1,-1,-1):
            val = val + Sum - n * nums[i]
            ans = max(ans,val)
        return ans
